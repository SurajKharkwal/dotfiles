{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "M3",
  "author": "Flyinghawk",
  "themes": [
    {
      "name": "Material",
      "appearance": "dark",
      "style": {
        "accents": [
          "{{colors.primary.hex}}",
          "{{colors.secondary.hex}}",
          "{{colors.tertiary.hex}}"
        ],

        "background.appearance": "opaque",

        "border": "{{colors.outlineVariant.hex}}",
        "border.variant": "{{colors.outline.hex}}",
        "border.focused": "{{colors.primary.hex}}",
        "border.selected": "{{colors.primary.hex}}",
        "border.transparent": "{{colors.outlineVariant.hex}}40",
        "border.disabled": "{{colors.outlineVariant.hex}}60",

        "elevated_surface.background": "{{colors.surfaceContainerHigh.hex}}",
        "surface.background": "{{colors.surface.hex}}",
        "background": "{{colors.background.hex}}",
        "element.background": "{{colors.surfaceContainer.hex}}",
        "element.hover": "{{colors.surfaceContainerHigh.hex}}",
        "element.active": "{{colors.surfaceContainerHighest.hex}}",
        "element.selected": "{{colors.secondaryContainer.hex}}",
        "element.disabled": "{{colors.surfaceVariant.hex}}",

        "drop_target.background": "{{colors.primaryContainer.hex}}80",

        "ghost_element.background": null,
        "ghost_element.hover": "{{colors.surfaceContainer.hex}}80",
        "ghost_element.active": "{{colors.surfaceContainerHigh.hex}}",
        "ghost_element.selected": "{{colors.secondaryContainer.hex}}80",
        "ghost_element.disabled": "{{colors.surfaceVariant.hex}}60",

        "text": "{{colors.onSurface.hex}}",
        "text.muted": "{{colors.onSurfaceVariant.hex}}",
        "text.placeholder": "{{colors.onSurfaceVariant.hex}}99",
        "text.disabled": "{{colors.onSurface.hex}}60",
        "text.accent": "{{colors.primary.hex}}",

        "icon": "{{colors.onSurface.hex}}",
        "icon.muted": "{{colors.onSurfaceVariant.hex}}",
        "icon.disabled": "{{colors.onSurface.hex}}60",
        "icon.placeholder": "{{colors.onSurfaceVariant.hex}}80",
        "icon.accent": "{{colors.primary.hex}}",

        "status_bar.background": "{{colors.surface.hex}}",
        "title_bar.background": "{{colors.surface.hex}}",
        "title_bar.inactive_background": "{{colors.surfaceDim.hex}}",
        "toolbar.background": "{{colors.surfaceContainerLow.hex}}",
        "tab_bar.background": "{{colors.surfaceContainer.hex}}",
        "tab.inactive_backgorround": "{{colors.surfaceContainerLow.hex}}",
        "tab.active_background": "{{colors.surfaceContainerHigh.hex}}",

        "search.match_background": "{{colors.tertiaryContainer.hex}}80",

        "panel.background": "{{colors.surfaceContainerLow.hex}}",
        "panel.focused_border": "{{colors.primary.hex}}",
        "pane.focused_border": "{{colors.primary.hex}}",

        "scrollbar.thumb.background": "{{colors.onSurfaceVariant.hex}}80",
        "scrollbar.thumb.hover_background": "{{colors.onSurfaceVariant.hex}}c0",
        "scrollbar.thumb.border": "{{colors.outlineVariant.hex}}40",
        "scrollbar.track.background": "{{colors.surfaceContainer.hex}}",
        "scrollbar.track.border": "{{colors.outlineVariant.hex}}20",

        "editor.foreground": "{{colors.onSurface.hex}}",
        "editor.background": "{{colors.surfaceContainerLow.hex}}",
        "editor.gutter.background": "{{colors.surfaceContainerLow.hex}}",
        "editor.subheader.background": "{{colors.surfaceContainer.hex}}",

        "editor.indent_guide": "{{colors.outlineVariant.hex}}60",
        "editor.indent_guide_active": "{{colors.outline.hex}}",

        "editor.active_line.background": "{{colors.surfaceContainerHigh.hex}}80",
        "editor.highlighted_line.background": "{{colors.surfaceContainerHigh.hex}}60",

        "editor.line_number": "{{colors.onSurfaceVariant.hex}}",
        "editor.active_line_number": "{{colors.primary.hex}}",

        "editor.invisible": "{{colors.outlineVariant.hex}}80",
        "editor.wrap_guide": "{{colors.outlineVariant.hex}}40",
        "editor.active_wrap_guide": "{{colors.outline.hex}}80",

        "editor.document_highlight.read_background": "{{colors.primaryContainer.hex}}60",
        "editor.document_highlight.write_background": "{{colors.secondaryContainer.hex}}80",

        "terminal.background": "{{colors.surfaceContainerLow.hex}}",
        "terminal.foreground": "{{colors.onSurface.hex}}",
        "terminal.bright_foreground": "{{colors.onSurface.hex}}",
        "terminal.dim_foreground": "{{colors.onSurfaceVariant.hex}}",

        "terminal.ansi.black": "{{colors.surfaceDim.hex}}",
        "terminal.ansi.bright_black": "{{colors.surfaceContainerHigh.hex}}",
        "terminal.ansi.dim_black": "{{colors.surface.hex}}",

        "terminal.ansi.red": "{{colors.error.hex}}",
        "terminal.ansi.bright_red": "{{colors.error.hex}}",
        "terminal.ansi.dim_red": "{{colors.onErrorContainer.hex}}",

        "terminal.ansi.green": "{{colors.tertiary.hex}}",
        "terminal.ansi.bright_green": "{{colors.tertiary.hex}}",
        "terminal.ansi.dim_green": "{{colors.onTertiaryContainer.hex}}",

        "terminal.ansi.yellow": "{{colors.tertiaryFixedDim.hex}}",
        "terminal.ansi.bright_yellow": "{{colors.tertiaryFixed.hex}}",
        "terminal.ansi.dim_yellow": "{{colors.onTertiaryFixed.hex}}",

        "terminal.ansi.blue": "{{colors.primary.hex}}",
        "terminal.ansi.bright_blue": "{{colors.primary.hex}}",
        "terminal.ansi.dim_blue": "{{colors.onPrimaryContainer.hex}}",

        "terminal.ansi.magenta": "{{colors.secondary.hex}}",
        "terminal.ansi.bright_magenta": "{{colors.secondary.hex}}",
        "terminal.ansi.dim_magenta": "{{colors.onSecondaryContainer.hex}}",

        "terminal.ansi.cyan": "{{colors.primaryFixedDim.hex}}",
        "terminal.ansi.bright_cyan": "{{colors.primaryFixed.hex}}",
        "terminal.ansi.dim_cyan": "{{colors.onPrimaryFixedVariant.hex}}",

        "terminal.ansi.white": "{{colors.onSurface.hex}}",
        "terminal.ansi.bright_white": "{{colors.onSurface.hex}}",
        "terminal.ansi.dim_white": "{{colors.onSurfaceVariant.hex}}",

        "link_text.hover": "{{colors.primary.hex}}",

        "conflict": "{{colors.error.hex}}",
        "conflict.background": "{{colors.errorContainer.hex}}80",
        "conflict.border": "{{colors.onErrorContainer.hex}}",

        "created": "{{colors.tertiary.hex}}",
        "created.background": "{{colors.tertiaryContainer.hex}}80",
        "created.border": "{{colors.onTertiaryContainer.hex}}",

        "deleted": "{{colors.error.hex}}",
        "deleted.background": "{{colors.errorContainer.hex}}80",
        "deleted.border": "{{colors.onErrorContainer.hex}}",

        "error": "{{colors.error.hex}}",
        "error.background": "{{colors.errorContainer.hex}}",
        "error.border": "{{colors.onErrorContainer.hex}}",

        "hidden": "{{colors.outlineVariant.hex}}",
        "hidden.border": "{{colors.outlineVariant.hex}}60",

        "hint": "{{colors.primary.hex}}",
        "hint.background": "{{colors.primaryContainer.hex}}80",
        "hint.border": "{{colors.onPrimaryContainer.hex}}",

        "ignored": "{{colors.onSurfaceVariant.hex}}60",
        "ignored.background": "{{colors.surfaceVariant.hex}}40",
        "ignored.border": "{{colors.outlineVariant.hex}}40",

        "info": "{{colors.primary.hex}}",
        "info.background": "{{colors.primaryContainer.hex}}80",
        "info.border": "{{colors.onPrimaryContainer.hex}}",

        "modified": "{{colors.secondary.hex}}",
        "modified.background": "{{colors.secondaryContainer.hex}}80",
        "modified.border": "{{colors.onSecondaryContainer.hex}}",

        "predictive": "{{colors.onSurfaceVariant.hex}}80",
        "predictive.border": "{{colors.outline.hex}}",
        "predictive.background": "{{colors.surfaceContainerHighest.hex}}80",

        "renamed": "{{colors.secondary.hex}}",
        "renamed.border": "{{colors.onSecondaryContainer.hex}}",
        "renamed.background": "{{colors.secondaryContainer.hex}}80",

        "success": "{{colors.tertiary.hex}}",
        "success.background": "{{colors.tertiaryContainer.hex}}80",
        "success.border": "{{colors.onTertiaryContainer.hex}}",

        "unreachable": "{{colors.onSurfaceVariant.hex}}60",
        "unreachable.background": "{{colors.surfaceVariant.hex}}40",
        "unreachable.border": "{{colors.outlineVariant.hex}}60",

        "warning": "{{colors.tertiaryFixedDim.hex}}",
        "warning.background": "{{colors.tertiaryContainer.hex}}80",
        "warning.border": "{{colors.onTertiaryContainer.hex}}",

        "players": [
          {
            "cursor": "{{colors.primary.hex}}",
            "background": "{{colors.primaryContainer.hex}}80",
            "selection": "{{colors.primaryContainer.hex}}60"
          },
          {
            "cursor": "{{colors.secondary.hex}}",
            "background": "{{colors.secondaryContainer.hex}}80",
            "selection": "{{colors.secondaryContainer.hex}}60"
          }
        ],

        "syntax": {
          "boolean": {
            "color": "{{colors.tertiary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "comment": {
            "color": "{{colors.onSurfaceVariant.hex}}",
            "font_style": "italic",
            "font_weight": null
          },
          "comment.doc": {
            "color": "{{colors.onSurfaceVariant.hex}}",
            "font_style": "italic",
            "font_weight": null
          },
          "constant": {
            "color": "{{colors.tertiary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "constructor": {
            "color": "{{colors.secondary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "emphasis": {
            "color": "{{colors.primary.hex}}",
            "font_style": "italic",
            "font_weight": null
          },
          "emphasis.strong": {
            "color": "{{colors.primary.hex}}",
            "font_style": null,
            "font_weight": 700
          },
          "function": {
            "color": "{{colors.primary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "keyword": {
            "color": "{{colors.secondary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "number": {
            "color": "{{colors.tertiaryFixed.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "operator": {
            "color": "{{colors.onSurfaceVariant.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "property": {
            "color": "{{colors.onSurface.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "punctuation": {
            "color": "{{colors.onSurfaceVariant.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.bracket": {
            "color": "{{colors.secondaryFixed.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.delimiter": {
            "color": "{{colors.onSurfaceVariant.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.list_marker": {
            "color": "{{colors.onSurfaceVariant.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.special": {
            "color": "{{colors.secondary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "string": {
            "color": "{{colors.tertiary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "string.escape": {
            "color": "{{colors.tertiaryFixedDim.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "string.regex": {
            "color": "{{colors.tertiaryFixed.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "string.special": {
            "color": "{{colors.onTertiaryContainer.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "string.special.symbol": {
            "color": "{{colors.tertiary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "tag": {
            "color": "{{colors.secondary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "text.literal": {
            "color": "{{colors.tertiary.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "type": {
            "color": "{{colors.primaryFixed.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "variable": {
            "color": "{{colors.onSurface.hex}}",
            "font_style": null,
            "font_weight": null
          },
          "variable.special": {
            "color": "{{colors.primary.hex}}",
            "font_style": null,
            "font_weight": null
          }
        }
      }
    }
  ]
}
